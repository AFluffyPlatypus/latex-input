(quail-define-package
 "<%= name.downcase %>" "<%= name %>" "<%= name.upcase[0..2] %>" t
 "Use <%= name %>  notation to enter Unicode characters.")

(quail-define-rules
<% notation_to_unicode.keys.sort.each do |notation| %>("<%= escape(notation)%> " ?<%= notation_to_unicode[notation] %>)
<% end %>)

(provide '<%= name.downcase %>)
