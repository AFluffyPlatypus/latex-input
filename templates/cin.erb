%gen_inp
%cname <%= name %>
%ename <%= name %>
%encoding UTF-8
%selkey @\abcdefghijklmnopqrstuvwxyz
%endkey 
%keyname begin
<% notation_to_unicode.keys.sort.each do |latex| %><%= latex %> <%= notation_to_unicode[latex] %>
<% end %>%keyname end
%chardef begin
<% notation_to_unicode.keys.sort.each do |latex| %><%= latex %> <%= notation_to_unicode[latex] %>
<% end %>%chardef end